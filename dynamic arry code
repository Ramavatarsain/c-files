//dynamic arry code
#include <iostream>
using namespace std;

int main() {
    int *arr;
    int a;

    cout << "Enter your value: ";
    cin >> a;

    arr = new int[a];

    for (int i = 0; i < a; i++) {
        cin >> arr[i];
    }

    for (int i = 0; i < a; i++) {
        cout << arr[i] << " ";
    }

    delete[] arr;
    return 0;
}



